@model SmartAdmin.WebUI.ViewModels.CategoryViewModel
<div class="row">
    <div class="col-xl-6">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    Category <span class="fw-300"><i>Details</i></span>
                </h2>
                <div class="panel-toolbar">
                    <button class="btn btn-panel" data-action="panel-collapse" data-toggle="tooltip" data-offset="0,10" data-original-title="Collapse"></button>
                    <button class="btn btn-panel" data-action="panel-fullscreen" data-toggle="tooltip" data-offset="0,10" data-original-title="Fullscreen"></button>
                    <button class="btn btn-panel" data-action="panel-close" data-toggle="tooltip" data-offset="0,10" data-original-title="Close"></button>
                </div>
            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <div class="panel-tag">
                        Category Details
                    </div>
                    <dl class="row">
                     
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.CategoryName)
                        </dt>
                        <dd class="col-sm-8">
                            @Html.DisplayFor(model => model.CategoryName)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.CategoryDesc)
                        </dt>
                        <dd class="col-sm-8">
                            @Html.DisplayFor(model => model.CategoryDesc)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.CatImage)
                        </dt>
                        <dd class="col-sm-8">
                                <img class="img-fluid"  src="~/Uploads/Category/@Model.CatImage" />
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.IsActive)
                        </dt>
                        <dd class="col-sm-8">
                                <input readonly class="form-check-input" asp-for="IsActive" /> 
                        </dd>
                      
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.CategoryKeyword)
                        </dt>
                        <dd class="col-sm-8">
                            @Html.DisplayFor(model => model.CategoryKeyword)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.CatSeq)
                        </dt>
                        <dd class="col-sm-8">
                            @Html.DisplayFor(model => model.CatSeq)
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>
</div>
